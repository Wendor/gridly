{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "src/env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.web.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "strict": false,
    "verbatimModuleSyntax": false
  }
}